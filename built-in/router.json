    "controller": "controller/table.ctrl.php"
  },
  {
    "path_pattern": "/^tabledata\\/(?P<database>[a-z_]+)\\/(?P<table>[a-z_]+)$/",
    "controller": "controller/tabledata.ctrl.php"
  },
  {
    "path_pattern": "/^company\\/search\\/autocomplete\\/(?P<filter>[a-z_]+)$/",
